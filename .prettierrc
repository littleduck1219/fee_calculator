{
  "singleQuote": false,
  "parser": "typescript",
  "semi": true,
  "useTabs": false,
  "tabWidth": 4,
  "trailingComma": "all",
  "printWidth": 100,
  "arrowParens": "always",
  "bracketSameLine": false,
  "plugins": [
    "prettier-plugin-style-order",
    "prettier-plugin-organize-imports"
  ]
}
